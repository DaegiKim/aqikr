GET     /                           controllers.HomeController.index(request: Request)
GET     /test                       controllers.HomeController.test
GET     /map                        controllers.HomeController.map
GET     /data                       controllers.HomeController.data
GET     /station                    controllers.HomeController.station(request: Request, stationName ?= null)
GET     /search                     controllers.HomeController.search(term ?= null)
GET     /searchByXY                 controllers.HomeController.searchByXY(request: Request, x ?= null, y ?= null)
GET     /color                      controllers.HomeController.getColor(stationName ?= null)

GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
